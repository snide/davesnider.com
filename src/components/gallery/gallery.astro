---
import { Image } from 'astro:assets';
const { images } = Astro.props;
console.log(images);
---

{
  images.map((image, index) => {
    return <Image src={image} width={1200} alt="Folder image" />;
  })
}
<style>
  img {
    max-width: 1200px !important;
    width: 100% !important;
    height: auto;
    margin-bottom: 0.5rem !important;
    margin-left: auto;
    margin-right: auto;
  }

  img:last-child {
    margin-bottom: 0;
  }
</style>
